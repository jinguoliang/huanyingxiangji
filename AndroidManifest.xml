<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.huanyingxiangji1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.front" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name=".MyApplication"
        android:icon="@drawable/android_huan"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- main -->
        <activity
            android:name=".activity.MainMenu"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- picture -->
        <activity
            android:name=".activity.PreviewAndPicture"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.DeviceDefault.Light.NoActionBar.Overscan" >
        </activity>
        <!-- picture Group -->
        <activity
            android:name=".activity.GroupList"
            android:label="@string/title_activity_main"
            android:theme="@android:style/Theme.DeviceDefault.Light.NoActionBar">
        </activity>
        <!-- picture view -->
        <activity
            android:name=".activity.ViewPicture"
            android:label="@string/title_activity_main" >
        </activity>
        <!-- create new Group -->
        <activity
            android:name=".activity.CreateNewGroup"
            android:label="@string/title_activity_main"
            android:theme="@android:style/Theme.DeviceDefault.Light.NoActionBar">
        </activity>
       <activity
            android:name=".activity.WorkSetActivity"
            android:label="@string/title_activity_main">
      </activity>
        
        
    </application>

</manifest>